<div class="wrapper fadeInDown">
    <div class="header">
        <h3>Dashboard</h3>
    </div>
    <table id="formContent" style="max-width: 600px;">
        <tr>
            <th>Date</th>
            <th>Start Time</th>
            <th>Duration</th>
            <th>Booked By</th>
            <th><a href="#">Add Multiple Time Slots</a></th>
        </tr>
        {{#each appointments}}
            <tr>
                <td>{{this.date}}</td>
                <td>{{this.startTime}}</td>
                <td>{{this.duration}}</td>
                <td>{{this.bookedBy}}</td>
                <td><button>Details</button></td>
                <td><button>Delete</button></td><br>
            </tr>
        {{/each}}
    </table>
</div>
